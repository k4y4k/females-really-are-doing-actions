{
  "name": "fancy-site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "vue-tsc --noEmit --skipLibCheck && vite build",
    "cy": "cypress open",
    "cy:ct": "cypress run-ct",
    "cy:run": "cypress run",
    "dev": "vite",
    "lint:eslint": "eslint src/**/*.{vue,ts} --fix",
    "lint:eslint:check": "eslint src/**/*.{vue,ts}",
    "lint:prettier": "prettier . --write",
    "lint:prettier:check": "prettier . --check",
    "lint:stylelint": "stylelint --fix './**/*.{css,vue}'",
    "lint:stylelint:check": "stylelint './**/*.{css,vue}'",
    "serve": "vite preview",
    "start": "vite",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "test:boot": "yarn build && yarn serve",
    "test:cy": "start-server-and-test test:boot http://localhost:5000 cy:run",
    "test:cy:ct": "start-server-and-test test:boot http://localhost:5000 cy:ct",
    "lint:types": "vue-tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "vue": "^3.2.16"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@cypress/vite-dev-server": "^2.1.1",
    "@cypress/vue": "3",
    "@mdx-js/react": "^1.6.22",
    "@storybook/addon-actions": "^6.4.0-beta.5",
    "@storybook/addon-docs": "^6.4.0-beta.5",
    "@storybook/addon-essentials": "^6.4.0-beta.5",
    "@storybook/addon-links": "^6.4.0-beta.5",
    "@storybook/vue3": "^6.4.0-beta.5",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.33.0",
    "@vitejs/plugin-vue": "^1.9.2",
    "babel-loader": "^8.2.2",
    "cypress": "^8.5.0",
    "eslint": "^7.12.1",
    "eslint-config-standard-with-typescript": "latest",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.0.0",
    "eslint-plugin-vue": "^7.19.1",
    "eslint-plugin-vue-a11y": "^0.0.31",
    "prettier": "^2.4.1",
    "sass": "^1.42.1",
    "start-server-and-test": "^1.14.0",
    "storybook-builder-vite": "^0.1.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "typescript": "^4",
    "vite": "^2.6.0",
    "vue-eslint-parser": "^7.11.0",
    "vue-loader": "^16.8.1",
    "vue-tsc": "^0.3.0"
  },
  "packageManager": "yarn@3.0.2"
}
