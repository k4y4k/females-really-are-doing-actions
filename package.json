{
  "name": "fancy-site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "vue-tsc --noEmit --skipLibCheck && vite build",
    "cy": "cypress open",
    "cy:ct": "cypress open-ct",
    "cy:run": "cypress run",
    "cy:run:ct": "cypress run-ct",
    "dev": "vite",
    "lint:eslint": "eslint src/**/*.{vue,ts} --fix",
    "lint:eslint:check": "eslint src/**/*.{vue,ts}",
    "lint:prettier": "prettier . --write",
    "lint:prettier:check": "prettier . --check",
    "lint:stylelint": "stylelint --fix './**/*.{css,vue}'",
    "lint:stylelint:check": "stylelint './**/*.{css,vue}'",
    "lint:types": "vue-tsc --noEmit --skipLibCheck",
    "serve": "vite preview",
    "start": "vite",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "test:boot": "yarn build && yarn serve",
    "test:cy": "start-server-and-test 'yarn test:boot' http://localhost:5000 'yarn cy:run'",
    "test:cy:ch": "start-server-and-test 'yarn test:boot' http://localhost:5000 'yarn cy:run --browser chrome'",
    "test:cy:ct": "start-server-and-test 'yarn test:boot' http://localhost:5000 'yarn cy:run:ct'",
    "test:cy:ct:ch": "start-server-and-test 'yarn test:boot' http://localhost:5000 'yarn cy:run:ct --browser chrome'",
    "test:cy:ct:ff": "start-server-and-test 'yarn test:boot' http://localhost:5000 'yarn cy:run:ct --browser firefox'",
    "test:cy:ff": "start-server-and-test 'yarn test:boot' http://localhost:5000 'yarn cy:run --browser firefox'"
  },
  "dependencies": {
    "@vue/compiler-core": "3.2.20",
    "@vue/compiler-dom": "3.2.20",
    "postcss": "^8.3.9",
    "vue": "3.2.19",
    "vue-router": "4"
  },
  "devDependencies": {
    "@babel/core": "7.15.8",
    "@cypress/code-coverage": "3.9.11",
    "@cypress/vite-dev-server": "2.1.1",
    "@cypress/vue": "3.0.3",
    "@mdx-js/react": "1.6.22",
    "@storybook/addon-actions": "6.4.0-beta.7",
    "@storybook/addon-docs": "6.4.0-beta.7",
    "@storybook/addon-essentials": "6.4.0-beta.7",
    "@storybook/addon-links": "6.4.0-beta.7",
    "@storybook/vue3": "6.4.0-beta.7",
    "@types/node": "16.10.3",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "@vitejs/plugin-vue": "1.9.3",
    "babel-loader": "8.2.2",
    "cypress": "8.5.0",
    "eslint": "8.0.0",
    "eslint-config-standard-with-typescript": "latest",
    "eslint-import-resolver-node": "0.3.6",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-vue": "7.19.1",
    "eslint-plugin-vue-a11y": "0.0.31",
    "nyc": "15.1.0",
    "postcss-windicss": "^1.0.0",
    "prettier": "2.4.1",
    "pug": "^3.0.2",
    "sass": "1.42.1",
    "start-server-and-test": "1.14.0",
    "storybook-builder-vite": "0.1.0",
    "stylelint": "13.13.1",
    "stylelint-config-standard": "22.0.0",
    "typescript": "4.4.3",
    "vite": "2.6.5",
    "vite-plugin-istanbul": "2.2.1",
    "vite-plugin-pug": "^0.3.0",
    "vscode-languageserver-textdocument": "1.0.2",
    "vue-eslint-parser": "7.11.0",
    "vue-loader": "16.8.1",
    "vue-tsc": "0.28.0",
    "windicss": "^3.1.9"
  },
  "packageManager": "yarn@3.0.2"
}
